{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vite-src/dist/",
  "url": "/",
  "port": 3000,
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "filesystem.*", "os.*", "debug.*", "storage.*", "extensions.*", "events.*"],
  "singlePageServe": true,
  "enableExtensions": true,
  "extensions": [{
    "id": "fileLoader",
    "command": "\"${NL_PATH}/extensions/extension.exe\" > extension_log.txt 2>&1"
  }],
  "modes": {
    "window": {
      "title": "VLauncher",
      "width": 800,
      "height": 600,
      "maxHeight": 600,
      "maxWidth": 800,
      "minHeight": 600,
      "minWidth": 800,
      "exitProcessOnClose": false,
      "resizable": false,
      "icon": "/vite-src/public/icon.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "VLauncher",
    "resourcesPath": "/vite-src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.2.0",
    "clientVersion": "6.2.0",
    "frontendLibrary": {
      "patchFile": "/vite-src/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/vite-src/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}