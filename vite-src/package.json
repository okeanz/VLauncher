{
	"name": "vlauncher",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "npm run prepare:extension && vite",
		"build:extension": "node esbuild.extension.config.mjs",
		"package:extension": "pkg --config ./pkg.json ./dist/extension.js --targets node18-win-x64 --output ../extensions/extension.exe",
		"prepare:extension": "npm run build:extension && npm run package:extension",
		"build": "tsc -b && vite build && npm run prepare:extension",
		"lint": "npm run lint:all",
		"lint:extension": "eslint extension/src/ --config extension/eslint.config.js --fix",
		"lint:all": "npm run lint:main && npm run lint:extension",
		"lint:main": "eslint src/ --fix",
		"preview": "vite preview"
	},
	"dependencies": {
		"@mantine/core": "^7.16.3",
		"@mantine/hooks": "^7.16.3",
		"@neutralinojs/lib": "5.4.0",
		"@reduxjs/toolkit": "2.5.1",
		"@tabler/icons-react": "^3.30.0",
		"adm-zip": "^0.5.16",
		"archiver": "^7.0.1",
		"axios": "^1.12.2",
		"clsx": "2.1.1",
		"crypto": "^1.0.1",
		"dotenv": "^17.2.2",
		"pidlockfile": "^1.1.1",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-error-boundary": "4.1.2",
		"react-redux": "9.2.0",
		"react-router": "7.1.5",
		"react-router-dom": "7.1.5",
		"tailwind-merge": "2.6.0",
		"ws": "^8.18.3"
	},
	"devDependencies": {
		"@types/adm-zip": "^0.5.7",
		"@types/node": "22.13.1",
		"@types/react": "19.0.8",
		"@types/react-dom": "19.0.3",
		"@types/websocket": "^1.0.10",
		"@types/ws": "^8.18.1",
		"@vitejs/plugin-react-swc": "3.8.0",
		"autoprefixer": "10.4.20",
		"copyfiles": "^2.4.1",
		"esbuild": "^0.25.10",
		"eslint": "9.20.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^15.9.0",
		"lefthook": "^1.12.2",
		"postcss": "^8.5.2",
		"postcss-preset-mantine": "^1.17.0",
		"prettier": "3.5.0",
		"rollup": "^4.46.2",
		"tailwindcss": "3.4.17",
		"typescript": "5.7.3",
		"typescript-eslint": "^8.44.0",
		"vite": "^5.0.0",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "2.1.9"
	},
	"engines": {
		"node": ">=18"
	}
}
